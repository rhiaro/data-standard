msgstr ""

#: ../../../schema/concepts.rst:17
#: 39f8d203e27b438ba96830a1c7b68dfe
msgid "Natural persons (who own or control entities)"
msgstr ""

#: ../../../schema/concepts.rst:20
#: 2391d779d83e464ca8b3956650d53735
msgid "Statements"
msgstr ""

#: ../../../schema/concepts.rst:22
#: f04bac9181b24e04abb9a430fac82f83
msgid "Details of how an interested party controls or owns a company or other legal entity are wrapped in an ownership-or-control statement."
msgstr ""

